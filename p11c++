#include <bits/stdc++.h>
using namespace std;
int main(int argc, char* argv[]) {
    if (argc != 4) {
        cout << "Usage: " << argv[0] << " num1 num2 num3\n";
        return 1;
    }
    
    int a = atoi(argv[1]);
    int b = atoi(argv[2]);
    int c = atoi(argv[3]);

    int largest =max(a,max(b,c));

    cout<< "Largest number: " << largest << "\n";
    return 0;
}
